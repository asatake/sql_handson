<h1>Listing Articles</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Body</th>
            <th>Author</th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        <%= for article <- @data do %>
            <tr>
                <td><%= article.title %></td>
                <td><%= article.body %></td>
                <td><%= article.author %></td>

                <td>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
